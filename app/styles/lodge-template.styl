#logo
	height: 100px;
	width: 100px;
	margin-left: 420px;

header
	min-width: 960px;
	background: #00539c; /* Old browsers */
	background: -moz-linear-gradient(top, #00539c 0%, #0061b5 25%, #007ce8 50%, #0061b5 75%, #00539c 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#00539c), color-stop(25%,#0061b5), color-stop(50%,#007ce8), color-stop(75%,#0061b5), color-stop(100%,#00539c)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top, #00539c 0%,#0061b5 25%,#007ce8 50%,#0061b5 75%,#00539c 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top, #00539c 0%,#0061b5 25%,#007ce8 50%,#0061b5 75%,#00539c 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top, #00539c 0%,#0061b5 25%,#007ce8 50%,#0061b5 75%,#00539c 100%); /* IE10+ */
	background: linear-gradient(to bottom, #00539c 0%,#0061b5 25%,#007ce8 50%,#0061b5 75%,#00539c 100%); /* W3C */
	border:5px solid grey;
	box-shadow: 2px 2px 6px 1px black;
	-webkit-border-radius: 100%;
	-webkit-border-bottom-right-radius: 5px;
	-webkit-border-bottom-left-radius: 5px;
	-moz-border-radius: 100%;
	-moz-border-radius-bottomright: 5px;
	-moz-border-radius-bottomleft: 5px;
	border-radius: 100%;
	border-bottom-right-radius: 5px;
	border-bottom-left-radius: 5px;
	h1
		color:white;
		text-transform: uppercase;
		text-align:center;
	h2
		color:white;
		font-variant: small-caps;
		text-align:center;
		font-size: 140%;
		font-style: italic;
		margin:-25px 0px 36px;
		text-shadow: 1px 1px 1px #BBB;

nav
	ul
		min-width: 960px;
		margin-left: 10px;
		padding-bottom: 10px;
	li
		display:inline-block;
		a.selected
			color:#cd7f32;
		a:hover
			color: gold;
		a:active
			color:#cd7f32;
		a
			margin:0 12px 0;
			padding:5px 18px 5px;
			background: #0350a3; /* Old browsers */
			background: -moz-linear-gradient(top, #0350a3 0%, #0350a3 11%, #0066c6 50%, #0350a3 88%, #0350a3 100%); /* FF3.6+ */
			background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#0350a3), color-stop(11%,#0350a3), color-stop(50%,#0066c6), color-stop(88%,#0350a3), color-stop(100%,#0350a3)); /* Chrome,Safari4+ */
			background: -webkit-linear-gradient(top, #0350a3 0%,#0350a3 11%,#0066c6 50%,#0350a3 88%,#0350a3 100%); /* Chrome10+,Safari5.1+ */
			background: -o-linear-gradient(top, #0350a3 0%,#0350a3 11%,#0066c6 50%,#0350a3 88%,#0350a3 100%); /* Opera 11.10+ */
			background: -ms-linear-gradient(top, #0350a3 0%,#0350a3 11%,#0066c6 50%,#0350a3 88%,#0350a3 100%); /* IE10+ */
			background: linear-gradient(to bottom, #0350a3 0%,#0350a3 11%,#0066c6 50%,#0350a3 88%,#0350a3 100%); /* W3C */
			border:3px groove #BBB;
			border-radius: 6%;
			color:white;
			text-decoration: none;
			font-variant: small-caps;
			font-size: 18px;
			font-weight: bold;
			box-shadow: 2px 2px 6px 1px black;
